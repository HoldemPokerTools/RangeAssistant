{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "properties": {
    "_id": {
      "maxLength": 32,
      "minLength": 16,
      "type": "string"
    },
    "actions": {
      "$id": "actions",
      "items": {
        "properties": {
          "color": {
            "pattern": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
            "type": "string"
          },
          "name": {
            "maxLength": 32,
            "minLength": 1,
            "type": "string"
          }
        },
        "required": ["name", "color"],
        "type": "object"
      },
      "maxItems": 25,
      "minItems": 2,
      "type": "array",
      "uniqueItems": true
    },
    "author": {
      "maxLength": 24,
      "minLength": 1,
      "type": "string"
    },
    "combos": {
      "$id": "combos",
      "additionalProperties": false,
      "maxProperties": 169,
      "minProperties": 1,
      "patternProperties": {
        "^([AKQJT2-9]{2})([os?])?$": {
          "items": {
            "type": "number"
          },
          "minItems": 1,
          "type": "array"
        }
      }
    },
    "title": {
      "maxLength": 48,
      "minLength": 1,
      "type": "string"
    }
  },
  "required": ["_id", "title", "actions", "combos"],
  "title": "Range",
  "type": "object"
}
